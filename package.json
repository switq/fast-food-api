{
  "name": "fast-food-api",
  "version": "1.0.0",
  "description": "Fast Food API with Express and Prisma",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:seed": "ts-node src/infrastructure/database/prisma/seed.ts",
    "db:seed:force": "ts-node src/infrastructure/database/prisma/seed-force.ts",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/uuid": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.4",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^29.5.0",
    "prisma": "^5.10.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "schema": "src/infrastructure/database/prisma/schema.prisma",
    "seed": "ts-node src/infrastructure/database/prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
